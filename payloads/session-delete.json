{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SessionDeletePayload",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": ["session.delete"],
      "description": "Type of payload - must be session.create"
    },
    "sessionId": {
      "type": "string",
      "description": "ID of the session to delete"
    }
  },
  "required": ["type", "sessionId"]
}

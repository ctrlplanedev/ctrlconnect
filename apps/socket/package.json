{
  "name": "@ctrlshell/socket",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "build": "tsc",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@ctrlshell/jest-presets/node"
  },
  "dependencies": {
    "@ctrlshell/logger": "workspace:*",
    "@t3-oss/env-core": "^0.11.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "prettier": "@ctrlshell/prettier-config",
  "devDependencies": {
    "@ctrlshell/eslint-config": "workspace:*",
    "@ctrlshell/jest-presets": "workspace:*",
    "@ctrlshell/prettier-config": "workspace:*",
    "@ctrlshell/typescript-config": "workspace:*",
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.3",
    "supertest": "^6.3.3",
    "typescript": "5.5.4"
  }
}
